    <form action="merchantsRegistered" enctype="multipart/form-data" method="post">
        <label>
            保千里ID：<input type="text" name="userbqlid" placeholder="保千里ID"> <br>
        </label>

        <label>
            商户手机：<input type="text" name="usermobile" placeholder="商户手机"> <br>
        </label>
        
        <label>
            商户名称：<input type="text" name="useralias" placeholder="用户名称"> <br>
        </label>

        <label>
            商户邮箱：<input type="text" name="useremail" placeholder="用户名称"> <br>
        </label>

        <label>
            商户密码：<input type="text" name="password1" placeholder="用户密码"> <br>
