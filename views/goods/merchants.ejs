        </label>

        <label>
            商户头像：<input type="text" name="userpic" placeholder="用户头像"> <br>
        </label>

        <label>
            商户性别：<input type="text" name="sex" placeholder="用户性别"> <br>
        </label>

        <label>
            商户生日：<input type="text" name="birthday" placeholder="用户性别"> <br>
        </label>

        <label>
            所在省地：<input type="text" name="province" placeholder="所在省地"> <br>
        </label>

        <label>
            所在市地：<input type="text" name="city" placeholder="所在市地"> <br>
        </label>

        <label>
            所在地区：<input type="text" name="area" placeholder="所在地区"> <br>
        </label>

        <label>
            地址备注：<input type="text" name="straddress" placeholder="地址备注" ><br>
        </label>

        <label>
            用户地址：<input type="text" name="address" placeholder="用户地址"><br>
        </label>

        <input type="submit" value="商户注册"><br>
    </form>

